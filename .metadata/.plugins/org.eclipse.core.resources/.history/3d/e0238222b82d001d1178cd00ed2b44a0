//--MontlyRanking 색변경-------------------------------------------------------------------------------------------

$(document).ready(function() {
	$(".indexRank_menu>div").click(function() {
		console.log(this);
		$(".indexRank_menu>div").css("color", "rgba(59, 0,148)");
		$(this).css("color", "rgba(255, 81,87)");
		//css("color: rgba(255, 81,87)"
	});
});
